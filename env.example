# Frontend Environment Configuration
# Copy this file to .env and fill in your actual values

# API Configuration
# Frontend (Expo/Web): use EXPO_PUBLIC_API_URL
EXPO_PUBLIC_API_URL=http://localhost:5000/api
API_TIMEOUT=10000

# Authentication
AUTH_TOKEN_KEY=fitness_app_auth_token
AUTH_REFRESH_TOKEN_KEY=fitness_app_refresh_token

# AI Services
OPENAI_API_KEY=your_openai_api_key_here
GOOGLE_VISION_API_KEY=your_google_vision_api_key_here

# Image Upload
CLOUDINARY_CLOUD_NAME=your_cloudinary_cloud_name
CLOUDINARY_API_KEY=your_cloudinary_api_key
CLOUDINARY_API_SECRET=your_cloudinary_api_secret

# Analytics
ANALYTICS_ENABLED=true
ANALYTICS_TRACKING_ID=your_analytics_tracking_id

# Development
DEBUG_MODE=true
LOG_LEVEL=debug

# Feature Flags
ENABLE_AI_FOOD_RECOGNITION=true
ENABLE_VOICE_WORKOUTS=true
ENABLE_SOCIAL_FEATURES=false
FEATURE_FEED=false
ENABLE_PUSH_NOTIFICATIONS=true

# App Configuration
APP_NAME=FitAI
APP_VERSION=1.0.0
BUILD_NUMBER=1
