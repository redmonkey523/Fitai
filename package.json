{
  "name": "fitness-app",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start --clear",
    "dev:web": "expo start --web",
    "dev:ios": "expo run:ios",
    "dev:android": "expo run:android",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "prebuild:ios": "npx expo prebuild --platform ios",
    "prebuild:android": "npx expo prebuild --platform android",
    "prebuild:clean": "npx expo prebuild --clean",
    "pod:install": "cd ios && pod install && cd ..",
    "xcode": "open ios/fitnessapp.xcworkspace",
    "ios:device": "npx expo run:ios --device",
    "ios:clean": "cd ios && xcodebuild clean -workspace fitnessapp.xcworkspace -scheme fitnessapp && cd ..",
    "build:ios": "eas build -p ios --profile production",
    "build:android": "eas build -p android --profile production",
    "test": "jest",
    "e2e:web": "playwright test",
    "e2e:mobile": "detox test",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:backend": "cd backend && npm test",
    "test:backend:watch": "cd backend && npm run test:watch",
    "test:backend:coverage": "cd backend && npm run test:coverage",
    "smoke-test": "node scripts/smoke-test.js",
    "e2e:smoke": "node scripts/e2e-smoke-test.js",
    "backend:start": "cd backend && npm start",
    "backend:dev": "cd backend && npm run dev",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "check:types": "tsc --noEmit",
    "validate": "node scripts/validate.js",
    "prepare": "husky install",
    "setup:xcode": "node scripts/setup-xcode.js"
  },
  "dependencies": {
    "@expo/metro-runtime": "~6.1.2",
    "@expo/vector-icons": "^15.0.2",
    "@ffmpeg/ffmpeg": "^0.12.15",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-community/netinfo": "^11.3.1",
    "@react-native-community/slider": "^5.0.1",
    "@react-native-firebase/analytics": "^23.4.0",
    "@react-native-firebase/app": "^23.4.0",
    "@react-native-firebase/crashlytics": "^23.4.0",
    "@react-native-google-signin/google-signin": "^15.0.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "@reduxjs/toolkit": "^2.8.2",
    "@sentry/react-native": "^6.22.0",
    "@tanstack/react-query": "^5.90.2",
    "axios": "^1.11.0",
    "babel-loader": "^10.0.0",
    "expo": "^54.0.12",
    "expo-av": "~16.0.7",
    "expo-barcode-scanner": "^13.0.1",
    "expo-camera": "~17.0.8",
    "expo-dev-client": "~6.0.13",
    "expo-document-picker": "~14.0.7",
    "expo-file-system": "~19.0.16",
    "expo-font": "~14.0.8",
    "expo-haptics": "^15.0.7",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linear-gradient": "~15.0.7",
    "expo-location": "~18.0.3",
    "expo-media-library": "~18.2.0",
    "expo-notifications": "^0.32.12",
    "expo-secure-store": "^15.0.7",
    "expo-sqlite": "^16.0.8",
    "expo-status-bar": "~3.0.8",
    "ffmpeg-kit-react-native": "^6.0.2",
    "html-webpack-plugin": "^5.6.4",
    "idb-keyval": "^6.2.2",
    "memoize-one": "5.2.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-native": "^0.81.4",
    "react-native-chart-kit": "^6.12.0",
    "react-native-draggable-flatlist": "^4.0.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-google-fit": "^0.19.0",
    "react-native-health": "^1.19.0",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "^15.12.1",
    "react-native-toast-message": "^2.3.3",
    "react-native-vector-icons": "^10.0.3",
    "react-native-vision-camera": "^4.7.1",
    "react-native-web": "^0.21.1",
    "react-native-worklets": "~0.5.1",
    "react-redux": "^9.2.0",
    "redux-persist": "^6.0.0",
    "socket.io-client": "^4.7.4",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-transform-class-properties": "^7.27.1",
    "@babel/plugin-transform-flow-strip-types": "^7.27.1",
    "@babel/plugin-transform-private-methods": "^7.27.1",
    "@babel/plugin-transform-private-property-in-object": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.9.0",
    "@types/react": "~19.1.10",
    "@typescript-eslint/eslint-plugin": "^8.4.0",
    "@typescript-eslint/parser": "^8.4.0",
    "eslint": "^9.1.0",
    "eslint-plugin-react": "^7.34.0",
    "husky": "^9.0.0",
    "jest": "^29.7.0",
    "jest-expo": "~54.0.12",
    "lint-staged": "^15.2.0",
    "prettier": "^3.3.0",
    "react-test-renderer": "^19.1.0",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier -w"
    ]
  },
  "private": true
}
